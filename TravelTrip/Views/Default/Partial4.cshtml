@using TravelTrip.Models.Classes
@model List<TravelTrip.Models.Blog>
<div class="padding1">
    <h4>Son əlavə edilənlər</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
        <a href="/Blog/BlogDetal/@x.Id">
            <div class="bot-img">
                <img src="~/@x.BlogImage" />
            </div>
            <div class="bot-desc">
                <p>@Html.Raw(x.Description.Substring(0, 50))</p>
                ...
            </div>
            <div class="clear"></div>
        </a>
        }
    </div>
    @Html.Action("Partial5", "Default")
</div>
