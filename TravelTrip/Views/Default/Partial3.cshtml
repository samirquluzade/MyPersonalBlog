@using TravelTrip.Models.Classes
@model List<TravelTrip.Models.Blog>
<div class="padding">
    <h4>This Week's Top 10</h4>
    <ul class="list list-pad">
        @{int i = 0;}
        @foreach(var x in Model)
        {
            <li class="list-bg"><span>@{i++;} @i.</span><a href="/Blog/BlogDetal/@x.Id">@x.Header</a></li>
        }
    </ul>
    <a href="/Blog/Index" class="link1">See all hotels</a>
</div>
